<h1 class="headinger"> Search Results </h1>

<% tabs.each do |tab| %>
  <div class="leaf">
  <p class="clicker" id="srch"> <%= highlight(tab.name, params[:search]) %></p>
    <div class="editor">
      <%= render "shared/buttons_tabs.html.erb", obj: tab %>
      <div class="creation">
        <%= render "shared/create_tab.html.erb", obj: tab %>
      </div>
    </div>
  </div>
<% end %>

{
  "manifest_version": 2,
  "name": "Tab Tree",
  "version": "0.1",
  "background": {
    "page": "tabby.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ]
}
